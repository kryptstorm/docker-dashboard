<div class="box">
  <div class="box-header with-border">
    <h3 class="box-title"><span class="label label-primary">{{images.length}} image(s)</span></h3>
  </div>
  <!-- /.box-header -->
  <div class="box-body">
    <table id="docker-images" class="table table-bordered table-striped">
      <thead>
        <tr>
          <th>Name</th>
          <th>Created At</th>
          <th>Size (MB)</th>
          <th class="text-center">Action</th>
        </tr>
      </thead>
      <tbody>
        {{#if images.length}}
          {{#each images}}
            <tr>
              <td>{{RepoTags}}</td>
              <td>{{secondToDatetime Created}}</td>
              <td>{{resolveSize Size}}</td>
              <td class="text-center">
                <a href="#" class="text-danger"><i class="fa fa-trash fw fa-lg"></i></a>
                <a href="#"><i class="fa fa-edit fw fa-lg"></i></a>
              </td>
            </tr>
          {{/each}}
        {{/if}}
      </tbody>
    </table>
  </div>
  <!-- /.box-body -->
</div>

